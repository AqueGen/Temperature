<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
        <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <connectionStrings>
    <add name="StoreContext" providerName="System.Data.SqlClient" connectionString="Data Source=(localdb)\TemperatureDB;Initial Catalog=TemperatureDB;Integrated Security=True;Connect Timeout=15;Encrypt=False;TrustServerCertificate=True;ApplicationIntent=ReadWrite;MultiSubnetFailover=False;" />
  </connectionStrings>

  <appSettings>
    <add key="TemperatureHttpServer" value="http://localhost:7747/?time=" />
  </appSettings>
</configuration>